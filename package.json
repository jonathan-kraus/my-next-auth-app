{
  "name": "my-neon-auth-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "next dev --turbopack",
    "build": "pnpm prisma generate && next build",
    "prettier:check": "prettier --check . --ignore-path .prettierignore",
    "prettier:fix": "prettier --write . --ignore-path .prettierignore",
    "security": "pnpm audit",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "prepare": "husky"
  },
  "pnpm": {
    "overrides": {
      "cookie": "^1.0.0"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
  ,

  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@axiomhq/js": "^1.3.1",
    "@axiomhq/logging": "^0.1.6",
    "@axiomhq/nextjs": "^0.1.6",
    "@axiomhq/react": "^0.1.6",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/serverless": "^1.0.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/adapter-neon": "^7.1.0",
    "@prisma/client": "7.1.0",
    "@prisma/client-runtime-utils": "^7.1.0",
    "@stackframe/stack": "2.8.56",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "autoprefixer": "^10.4.22",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.26",
    "husky": "^9.1.7",
    "mailersend": "^2.6.0",
    "motion": "^12.23.26",
    "next": "^16.0.9",
    "next-auth": "^4.24.13",
    "prettier": "^3.7.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hot-toast": "^2.6.0",
    "sonner": "^2.0.7",
    "sparkline": "^0.2.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.0.9",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prisma": "7.1.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "type": "module"
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501"
}
