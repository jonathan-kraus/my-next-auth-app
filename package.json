{
  "name": "my-neon-stack-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "next dev --turbopack",
    "build": "prisma generate --no-hints && next build",
    "prettier:check": "prettier --check . --ignore-path .prettierignore",
    "prettier:fix": "prettier --write . --ignore-path .prettierignore",
    "security": "pnpm audit",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@neondatabase/serverless": "1.0.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/adapter-neon": "7.1.0",
    "@prisma/client": "7.1.0",
    "@prisma/client-runtime-utils": "^7.1.0",
    "@stackframe/stack": "2.8.54",
    "autoprefixer": "^10.4.22",
    "next": "15.1.9",
    "next-auth": "^4.24.13",
    "prettier": "^3.7.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "prisma": "7.1.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18",
    "type": "module"
  }
}
