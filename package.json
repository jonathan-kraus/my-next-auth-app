{
  "name": "my-neon-auth-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "next start",
    "dev": "next dev --turbopack",
    "build": "pnpm prisma generate && next build",
    "prettier:check": "prettier --check . --ignore-path .prettierignore",
    "prettier:fix": "prettier --write . --ignore-path .prettierignore",
    "security": "pnpm audit",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "prepare": "husky"
  },
  "pnpm": {
    "overrides": {
      "cookie": "^1.0.0"
    },
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "bcrypt",
      "core-js",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@axiomhq/js": "^1.3.1",
    "@axiomhq/logging": "^0.1.6",
    "@axiomhq/nextjs": "^0.1.6",
    "@axiomhq/react": "^0.1.6",
    "@heroicons/react": "^2.2.0",
    "@neondatabase/neon-auth-next": "0.1.0-alpha.24",
    "@neondatabase/neon-auth-ui": "0.1.0-alpha.2",
    "@neondatabase/serverless": "^1.0.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/adapter-neon": "^7.2.0",
    "@prisma/client": "7.2.0",
    "@prisma/client-runtime-utils": "^7.2.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "autoprefixer": "^10.4.22",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.26",
    "husky": "^9.1.7",
    "lucide-react": "^0.562.0",
    "mailersend": "^2.6.0",
    "motion": "^12.23.26",
    "next-auth": "^4.24.11",
    "prettier": "^3.7.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hot-toast": "^2.6.0",
    "sonner": "^2.0.7",
    "sparkline": "^0.2.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.1.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "lint-staged": "^16.2.7",
    "next": "^16.1.0",
    "postcss": "^8.5.6",
    "prisma": "7.2.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.15"
  },
  "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa"
